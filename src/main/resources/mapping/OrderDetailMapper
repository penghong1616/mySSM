<?xml version="1.0" encoding="UTF-8" ?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <!-- namespace命名空间,作用就是对sql进行分类化的管理,理解为sql隔离
            注意:使用mapper代理开发时，namespace有特殊作用,namespace等于mapper接口地址
         -->
<mapper namespace="com.wsk.dao.OrderDetailMapper">
    <insert id="insert" parameterType="com.wsk.pojo.OrderDetail">
        insert into orderdetail(order_id,shop_info_id,shop_info_num)
        values(#{orderId},#{shopInfoId},#{shopInfoNum});
    </insert>
</mapper>